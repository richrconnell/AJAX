DROP TABLE VITAMINS;
DROP TABLE VITAMINSTORE;
DROP TABLE BRAND;

CREATE TABLE VITAMINS 
(VITAMINID INTEGER AUTO_INCREMENT, 
NAME VARCHAR(40),
BENEFITS VARCHAR(40), 
DOSAGE VARCHAR(40),
BRANDID INTEGER,
PRIMARY KEY (VITAMINID));

CREATE TABLE VITAMINSTORE 
(STOREID INTEGER AUTO_INCREMENT, 
STORENAME VARCHAR(40),
LOCATION VARCHAR(40), 
PRIMARY KEY (STOREID));

CREATE TABLE BRAND 
(BRANDID INTEGER AUTO_INCREMENT, 
BRANDNAME VARCHAR(40),
STOREID INTEGER,
PRIMARY KEY (BRANDID));

source ~connellr394/public_html/vitamins.sql;
source ~connellr394/public_html/vitaminstore.sql;
source ~connellr394/public_html/brand.sql;